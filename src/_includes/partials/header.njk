<header>
  {# <a href="/" id='logo' aria-label="west warwicks logo">
    {% set icon = cms.icons.favicon %}
    <img aria-hidden="true" src="{{icon.url}}" width="64" height="64" alt="{{icon.description}}" loading="lazy">
  </a> #}
  {% include "./logo.njk" %}

  <a id='mobile-menu' class='mobile-menu' href="#primary-navigation" aria-label="mobile menu">
    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-list" viewBox="0 0 16 16">
      <path fill-rule="evenodd" d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z"/>
    </svg>
  </a>

  {% set links = [
    "home",
    "sports",
    "bar",
    "restaurant",
    "events",
    "about",
    "contact"
  ] %}

  <nav id="primary-navigation">
    <ul>
      {% for link in links %}
        <li>
          <a href="/{{link}}">{{link}}</a>
        </li>
      {% endfor %}
    </ul>
    <a href="#mobile-menu" id='close-mobile-menu'>
      <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-x-circle" viewBox="0 0 16 16">
        <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z"/>
        <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z"/>
      </svg>
    </a>
  </nav>

  {% include "partials/theme-toggler.njk" %}
</header>