---js
{
  title: 'West Warwicks',
  site: 'https://westwarwicks.club',
  company: 'West Warwickshire Sports Complex',
  description: 'West Warwickshire Sports Complex - the heart of sport & hospitality in Solihull.'
}
---
{% import "utilities/macro.njk" as util %}

<!DOCTYPE html>
<html lang="en">

  <head>
    <title>{{ title if title else meta.title }}</title>

    {# deliberately early non-deferred use of theme toggle to render the color-scheme correctly while loading #}

    <script type="module" src="/@input/js/theme-toggle.js"></script>

    <meta charset="UTF-8"/>
    {% if build.production %}
      <base href="{{ site if site else meta.site }}/" target="_blank">
    {% endif %}

    <meta name="description" content="{{ description if description else meta.description }}">
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Google / Search Engine Tags -->
    <meta itemprop="name" content="{{ meta.company }}">
    <meta itemprop="description" content="{{ description if description else meta.description }}">
    <meta itemprop="image" content="{{ site if site else meta.site }}/social-image.png">

    <!-- Facebook Meta Tags -->
    <meta property="og:url" content="{{ site if site else meta.site }}">
    <meta property="og:type" content="website">
    <meta property="og:title" content="{{ company if company else meta.company }}">
    <meta property="og:description"
    content="description if description else meta.description ">
    <meta property="og:image" content="{{ site if site else meta.site }}/social-image.png">

    <!-- Twitter Meta Tags -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="{{ company if company else meta.company }}">
    <meta name="twitter:description"
    content="{{ description if description else meta.description }}">
    <meta name="twitter:image" content="{{ site if site else meta.site }}/social-image.png">

    <!-- favicon  -->
    <!-- https://evilmartians.com/chronicles/how-to-favicon-in-2021-six-files-that-fit-most-needs -->
    <link rel="icon" href="/favicon.ico" sizes="any">
    <link rel="icon" href="/favicon.svg" type="image/svg+xml">
    <!-- 32×32 -->
    <link rel="apple-touch-icon" type="image/png" href="/favicon-180.png">
    <!-- 180×180 -->

    <!-- manifest -->
    <link rel="manifest" href="/manifest.webmanifest">

    <link rel="stylesheet" href="/@input/sass/styles.scss">
  </head>

  <body>
    <a href="#main" class="skip-to-content">Skip to content</a>
    <div class="site container">
      <header>
        <a href="/" id='logo'>
          <img src="/favicon.svg" alt="site logo">
        </a>

        <button id='mobile-menu'>
          <a  class='mobile-menu' href="#primary-navigation" tabindex=-1>
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="menu bi bi-list" viewBox="0 0 16 16">
              <path fill-rule="evenodd" d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z"/>
            </svg>
          </a>
        </button>

        <nav id="primary-navigation">
          <ul>
            <li>
              <a href="#sports">sports</a>
            </li>
            <li>
              <a href="#bar">bar</a>
            </li>
            <li>
              <a href="#restaurant">restaurant</a>
            </li>
            <li>
              <a href="#events">events</a>
            </li>
            <li>
              <a href="#about">about</a>
            </li>
            <li>
              <a href="#about">contact</a>
            </li>
            <a href="#mobile-menu" id='close-mobile-menu'>
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-x-circle" viewBox="0 0 16 16">
                <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z"/>
                <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z"/>
              </svg>
            </a>
          </ul>
        </nav>
        <section id="theme-toggler">
          <button class='theme-toggle' data-theme="light">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-brightness-high-fill" viewBox="0 0 16 16">
              <path d="M12 8a4 4 0 1 1-8 0 4 4 0 0 1 8 0zM8 0a.5.5 0 0 1 .5.5v2a.5.5 0 0 1-1 0v-2A.5.5 0 0 1 8 0zm0 13a.5.5 0 0 1 .5.5v2a.5.5 0 0 1-1 0v-2A.5.5 0 0 1 8 13zm8-5a.5.5 0 0 1-.5.5h-2a.5.5 0 0 1 0-1h2a.5.5 0 0 1 .5.5zM3 8a.5.5 0 0 1-.5.5h-2a.5.5 0 0 1 0-1h2A.5.5 0 0 1 3 8zm10.657-5.657a.5.5 0 0 1 0 .707l-1.414 1.415a.5.5 0 1 1-.707-.708l1.414-1.414a.5.5 0 0 1 .707 0zm-9.193 9.193a.5.5 0 0 1 0 .707L3.05 13.657a.5.5 0 0 1-.707-.707l1.414-1.414a.5.5 0 0 1 .707 0zm9.193 2.121a.5.5 0 0 1-.707 0l-1.414-1.414a.5.5 0 0 1 .707-.707l1.414 1.414a.5.5 0 0 1 0 .707zM4.464 4.465a.5.5 0 0 1-.707 0L2.343 3.05a.5.5 0 1 1 .707-.707l1.414 1.414a.5.5 0 0 1 0 .708z"/>
            </svg>
          </button>
          <button class='theme-toggle' data-theme="dark">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-moon-stars-fill" viewBox="0 0 16 16">
              <path d="M6 .278a.768.768 0 0 1 .08.858 7.208 7.208 0 0 0-.878 3.46c0 4.021 3.278 7.277 7.318 7.277.527 0 1.04-.055 1.533-.16a.787.787 0 0 1 .81.316.733.733 0 0 1-.031.893A8.349 8.349 0 0 1 8.344 16C3.734 16 0 12.286 0 7.71 0 4.266 2.114 1.312 5.124.06A.752.752 0 0 1 6 .278z"/>
              <path d="M10.794 3.148a.217.217 0 0 1 .412 0l.387 1.162c.173.518.579.924 1.097 1.097l1.162.387a.217.217 0 0 1 0 .412l-1.162.387a1.734 1.734 0 0 0-1.097 1.097l-.387 1.162a.217.217 0 0 1-.412 0l-.387-1.162A1.734 1.734 0 0 0 9.31 6.593l-1.162-.387a.217.217 0 0 1 0-.412l1.162-.387a1.734 1.734 0 0 0 1.097-1.097l.387-1.162zM13.863.099a.145.145 0 0 1 .274 0l.258.774c.115.346.386.617.732.732l.774.258a.145.145 0 0 1 0 .274l-.774.258a1.156 1.156 0 0 0-.732.732l-.258.774a.145.145 0 0 1-.274 0l-.258-.774a1.156 1.156 0 0 0-.732-.732l-.774-.258a.145.145 0 0 1 0-.274l.774-.258c.346-.115.617-.386.732-.732L13.863.1z"/>
            </svg>
          </button>
        </section>
