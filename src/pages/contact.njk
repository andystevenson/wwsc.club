<h1>{{ title | capitalize }}</h1>

<section class="enquiries">
  <article data-theme="light">
    <h2>General Enquiries</h2>
    <section>
      <a href="mailto:{{email}}?Subject=General Enquiry">
        <img src="/images/envelope.svg" alt="email">
      </a>
      <a href="tel:{{telephone}}">
        <img src="/images/telephone.svg" alt="telephone">
      </a>
    </section>
  </article>
  <article data-theme="light">
    <header>
      <h2>About Joining</h2>
    </header>
    <section>
      <a href="mailto:{{email}}?Subject=About Joining">
        <img src="/images/envelope.svg" alt="email">
      </a>
      <a href="tel:{{telephone}}">
        <img src="/images/telephone.svg" alt="telephone">
      </a>
    </section>
  </article>
  <article data-theme="light">
    <header>
      <h2>Host an Event</h2>
    </header>
    <section>
      <a href="mailto:{{email}}?Subject=Host an Event">
        <img src="/images/envelope.svg" alt="email">
      </a>
      <a href="tel:{{telephone}}">
        <img src="/images/telephone.svg" alt="telephone">
      </a>
    </section>
  </article>
  <article data-theme="light">
    <header>
      <h2>Already a Member</h2>
    </header>
    <section>
      <a href="mailto:{{email}}?Subject=Member Enquiry">
        <img src="/images/envelope.svg" alt="email">
      </a>
      <a href="tel:{{telephone}}">
        <img src="/images/telephone.svg" alt="telephone">
      </a>
    </section>
  </article>
</section>

<form action="/opening-times" data-theme="light">
  <button>
    <h2>Opening times</h2>
    <img src="/images/clock.svg" class="clock" alt="">
  </button>
</form>

<section class="find-us">
  <h2>Find us</h2>
  <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2432.7621604957194!2d-1.8089166842588886!3d52.42910705071379!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x4870b98bd12f5729%3A0x9b13324877249de4!2sWest%20Warwickshire%20Sports%20Complex!5e0!3m2!1sen!2suk!4v1645210891788!5m2!1sen!2suk" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy">
    <span></span></iframe>
</section>

<section class="address">
  <p>West Warwickshire Sports Complex</p>
  <p>78 Grange Road</p>
  <p>Solihull</p>
  <p>B91 1DA</p>
  <a href="tel:{{ telephone }}" title="telephone {{ telephone }}">Tel: {{telephone}}</a>
  <a href="mailto:{{email}}" title="email {{email}}">Email: {{email}}</a>

</section>