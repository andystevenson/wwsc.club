{
  "name": "wwsc.club",
  "version": "1.0.1",
  "description": "new westwarwicks.co.uk website",
  "main": "index.html",
  "scripts": {
    "init": "npm link",
    "start": "run-s build-cache dev",
    "dev": "slinkity --serve --incremental",
    "build": "run-s build-cache production",
    "production": "slinkity",
    "clean": "rm -rf _site .11ty* ./public/cache/*/*",
    "clean-cache": "concurrently --timings 'npm:clean:*'",
    "clean:cache-stripe": "rm -rf ./public/cache/stripe/*",
    "clean:cache-opening-times": "rm -rf ./public/cache/opening-times/*",
    "clean:cache-sumup": "rm -rf ./public/cache/sumup/*",
    "build-cache": "concurrently --timings 'npm:build:*'",
    "build:cache-stripe": "npx cache-stripe",
    "build:cache-opening-times": "npx cache-opening-times",
    "build:cache-sumup": "npx cache-sumup",
    "clean-build": "run-s clean-cache build-cache"
  },
  "bin": {
    "cache-stripe": "./scripts/cache-stripe.js",
    "cache-opening-times": "./scripts/cache-opening-times.js",
    "cache-sumup": "./scripts/cache-sumup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andystevenson/wwsc.club.git"
  },
  "keywords": [
    "website",
    "westwarwicks",
    "html",
    "javascript",
    "css",
    "netlify"
  ],
  "author": "andystevenson@mac.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andystevenson/wwsc.club/issues"
  },
  "homepage": "https://github.com/andystevenson/wwsc.club#readme",
  "devDependencies": {
    "@andystevenson/goodtill": "^0.1.4",
    "autoprefixer": "^10.4.8",
    "commander": "^9.4.0",
    "concurrently": "^7.3.0",
    "cssnano": "^5.1.13",
    "download": "^8.0.0",
    "node-fetch": "^3.2.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-custom-media": "^8.0.0",
    "postcss-jit-props": "^1.0.4",
    "sass": "^1.49.0",
    "slinkity": "^0.6.1",
    "vite-plugin-minify": "^1.5.2"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@andystevenson/11ty": "^0.1.3",
    "@andystevenson/lib": "^0.2.0",
    "@andystevenson/sass": "^0.1.2",
    "@andystevenson/theme-toggle": "^0.1.5",
    "axios": "^0.27.2",
    "dayjs": "^1.11.4",
    "googleapis": "^104.0.0",
    "lodash": "^4.17.21",
    "netlify-cli": "^11.5.0",
    "nodemailer": "^6.7.6",
    "open-props": "^1.3.3",
    "stripe": "^8.210.0",
    "xlsx": "^0.18.2"
  }
}
